class Solution {
public:
    int findKthPositive(vector<int>& arr, int k) {
        int visited[1001]={0};
        int count = 0;
        for(auto i : arr){
            visited[i] = 1;
        }
        int i;
        for(i = 1;i<1001;i++){
            if(visited[i] != 1)
                ++count;
            if(count == k)
                return i;
        }
        return k-count+i-1;
    }
};
